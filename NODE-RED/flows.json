        "name": "function 1",
        "func": "let data = msg.payload;\n\n// Se payload for string JSON, parseia\nif (typeof data === \"string\") {\n    try {\n        data = JSON.parse(data);\n    } catch (e) {\n        node.error(\"Payload n√£o √© JSON v√°lido\");\n        return null;\n    }\n}\n\nlet linhas = [];\n\n// N√≠vel da √°gua\nif (typeof data.distancia === \"number\") {\n    if (data.distancia < 10) {\n        linhas.push(\"üö® N√≠vel de √°gua: PERIGOSO!\");\n    } else if (data.distancia < 20) {\n        linhas.push(\"‚úÖ N√≠vel de √°gua: NORMAL\");\n    } else {\n        linhas.push(\"‚ö†Ô∏è N√≠vel de √°gua: BAIXO\");\n    }\n} else {\n    linhas.push(\"‚ö†Ô∏è Dist√¢ncia n√£o dispon√≠vel\");\n}\n\n// Chuva\nif (typeof data.ldr === \"number\") {\n    if (data.ldr < 1000) {\n        linhas.push(\"üåß Grande chance de chuva!\");\n    } else if (data.ldr < 3000) {\n        linhas.push(\"‚òÅÔ∏è Baixa chance de chuva.\");\n    } else {\n        linhas.push(\"üåû Sem chance de chuva.\");\n    }\n} else {\n    linhas.push(\"‚ö†Ô∏è LDR n√£o dispon√≠vel\");\n}\n\n// Temperatura\nif (typeof data.temperatura === \"number\") {\n    let tempNum = data.temperatura;\n    if (tempNum >= 30) {\n        linhas.push(\"üî• Temperatura alta: \" + tempNum.toFixed(1) + \"¬∞C\");\n    } else if (tempNum >= 20) {\n        linhas.push(\"üå§ Temperatura agrad√°vel: \" + tempNum.toFixed(1) + \"¬∞C\");\n    } else {\n        linhas.push(\"‚ùÑÔ∏è Temperatura baixa: \" + tempNum.toFixed(1) + \"¬∞C\");\n    }\n} else {\n    linhas.push(\"‚ö†Ô∏è Temperatura n√£o dispon√≠vel\");\n}\n\n// Monta o HTML com cada linha dentro de uma div, para quebrar linha visual\nmsg.payload = `\n<div style=\"\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 100%;\n    text-align: center;\n    font-size: 18px;\n    flex-direction: column;\n    gap: 8px;\n\">\n    ${linhas.map(l => `<div>${l}</div>`).join('')}\n</div>\n`;\n\nreturn msg;\n",
